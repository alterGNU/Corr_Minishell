echo "TESTS 0:EXPORT AND UNSET UNVALID IDENTIFIER"
export v1=valid1 to.to=tata v2=valid2
echo $? v1=$v1, v2=$v2.
export 'u'=tata v3=valid3
echo $? v3=$v3.
